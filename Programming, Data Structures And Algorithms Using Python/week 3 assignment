def remdup(l):
  unique=[]
  for i in l:
    if i not in unique:
      unique.append(i)
  return(unique)
def sumsquare(a):
  oe=[0,0]
  for i in a:
    if i%2==0:
      oe[1]=oe[1]+i*i
    else:
      oe[0]=oe[0]+i*i
  return(oe)
def transpose(m):
  matrix=[]
  for i in range(len(m[0])):
    a=[]
    for j in range(len(m)):
      a.append(m[j][i])
    matrix.append(a)
  return(matrix)
  
